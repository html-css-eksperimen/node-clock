{
  "name": "nodeclocksampel",
  "version": "1.0.0",
  "description": "Boilerplate for JavaScript and TypeScript project from scratch, with ESLint/TSLint AirBNB Style.",
  "main": "app.js",
  "scripts": {
    "run": "parcel -p 4200 src/index.html",
    "build": "parcel build src/index.html --no-source-maps",
    "deploygithub": "parcel build src/index.html --no-source-maps --public-url /your-github-project-name/",
    "removedist": "rm -rf ./dist/*",
    "webbuild": "webpack --config webpack.prod.js",
    "buildweb": "webpack --config webpack.config.prod.js",
    "testprod": "webpack-dev-server --config webpack.config.prod.js",
    "testdebug": "webpack-dev-server --config webpack.config.dev.js",
    "mergedebug": "webpack-dev-server --config webpack.dev.js",
    "mergebuild": "webpack --config webpack.prod.js"
  },
  "keywords": [
    "boilerplate"
  ],
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "gulajava_ministudio",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.0",
    "css-minimizer-webpack-plugin": "^1.1.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.12.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.0.0",
    "typescript": "^3.9.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.1",
    "@popperjs/core": "^2.4.4",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "moment": "^2.27.0"
  }
}
